{
	"name": "nextjs-app",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"format": "prettier --write . && next lint",
		"db:generate": "dotenv -e .env.local -- prisma generate",
		"db:push": "dotenv -e .env.local -- prisma db push",
		"db:dev": "dotenv -e .env.local -- prisma migrate dev",
		"db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
		"db:studio": "dotenv -e .env.local -- prisma studio"
	},
	"dependencies": {
		"@iconify/react": "4.1.1",
		"@next-auth/prisma-adapter": "1.0.7",
		"@prisma/client": "5.16.1",
		"next": "14.2.4",
		"next-auth": "4.24.7",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "20",
		"@types/react": "18",
		"@types/react-dom": "18",
		"@typescript-eslint/eslint-plugin": "7.14.1",
		"@typescript-eslint/parser": "7.14.1",
		"dotenv-cli": "7.4.2",
		"eslint": "8",
		"eslint-config-next": "14.2.4",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-react": "7.34.1",
		"playwright": "1.45.1",
		"postcss": "8",
		"prettier": "3.2.5",
		"prettier-plugin-tailwindcss": "0.5.14",
		"prisma": "5.16.1",
		"tailwindcss": "3.4.1",
		"typescript": "5"
	}
}
